{
  "version": 2,
  "name": "ukwata-website",
  "builds": [
    { "src": "src/*.html", "use": "@vercel/static" },
    { "src": "css/*.css", "use": "@vercel/static" },
    { "src": "js/*.js", "use": "@vercel/static" },
    { "src": "images/*", "use": "@vercel/static" }
  ],
  "routes": [
    { "src": "/", "dest": "/src/main.html" }
  ]
}
